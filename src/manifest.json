{
  "name": "Screenshot CE",
  "version": "1.0",
  "description": "Utility to automate the process of take screenshot in multiple resolutions",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js", "runtime.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "downloads",
    "debugger",
    "storage"
  ]
}
